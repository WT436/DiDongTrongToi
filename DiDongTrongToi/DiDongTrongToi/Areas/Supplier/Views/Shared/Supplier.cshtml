                            }
                        }
                    </div>
                </li>
                <li class="nav-item dropdown navStyle " style="font-size: 80%;">
                    <a class="nav-link dropdown-toggle " href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="fa fa-2x fa-id-card"></i><br />Sim Thẻ
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="@Url.Action("Watch","Home")">Đồng Hồ Chung</a>
                        <div class="dropdown-divider"></div>
                        @{
                            var DanhMucCard = db.Danh_Muc_Theo_Chung_loai.Where(n => n.Danh_Muc_Theo_Chu_De == 6).ToList();
                            foreach (var ik in DanhMucCard)
                            {
