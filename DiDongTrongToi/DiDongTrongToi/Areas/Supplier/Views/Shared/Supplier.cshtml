                            }
                        }
                    </div>
                </li>
                <li class="nav-item dropdown navStyle " style="font-size: 80%;">
                    <a class="nav-link dropdown-toggle " href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="fa fa-2x fa-headphones"></i><br />Phụ Kiện
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="@Url.Action("accessories","Home")">Phụ Kiện Chung</a>
                        <div class="dropdown-divider"></div>
                        @{
                            var DanhMucaccessories = db.Danh_Muc_Theo_Chung_loai.Where(n => n.Danh_Muc_Theo_Chu_De == 4).ToList();
                            foreach (var ik in DanhMucaccessories)
                            {
