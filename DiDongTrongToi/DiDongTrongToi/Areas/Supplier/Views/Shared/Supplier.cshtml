                    </div>
                </li>
                <li class="nav-item dropdown navStyle " style="font-size: 80%;">
                    <a class="nav-link dropdown-toggle " href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="fa fa-2x fa-laptop"></i><br />Máy Tính
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="@Url.Action("Computer","Home")">Máy Tính Chung</a>
                        <div class="dropdown-divider"></div>
                        @{
                            var DanhMucComputer = db.Danh_Muc_Theo_Chung_loai.Where(n => n.Danh_Muc_Theo_Chu_De == 3).ToList();
                            foreach (var ik in DanhMucComputer)
                            {
