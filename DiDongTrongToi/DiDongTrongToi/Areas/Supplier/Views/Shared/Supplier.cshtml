        <a class="navbar-brand" href="@Url.Action("Index","Home")">
            <img src="~/Content/Home/img/WIN_20191213_01_15_11_Pro.jpg" style="height: 3em; width: 12em;" alt="...">
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon " style="color: black;"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active navStyle " style="font-size: 80%;">
                    <a class="nav-link " href="@Url.Action("Index","Home")">
                        <i class="fa fa-2x fa-home"></i><br />
                        Home
                        <span class="sr-only">(current)</span>
                    </a>
                </li>

                <li class="nav-item dropdown navStyle " style="font-size: 80%;">
                    <a class="nav-link dropdown-toggle " href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="fa fa-2x fa-mobile-phone"></i><br /> Điện Thoại
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="@Url.Action("Phone","Home")">Điện Thoại Chung</a>
                        <div class="dropdown-divider"></div>
                        @{
                            var DanhMucDienThoai = db.Danh_Muc_Theo_Chung_loai.Where(n => n.Danh_Muc_Theo_Chu_De == 1).ToList();
                            foreach (var ik in DanhMucDienThoai)
                            {
                                <a class="dropdown-item" href="@Url.Action("Phone","Home", new {id=ik.Danh_Muc_Theo_Chu_De})">@ik.ten_chung_Loai</a>
                            }
                        }
                    </div>
                </li>
                <li class="nav-item dropdown navStyle " style="font-size: 80%;">
