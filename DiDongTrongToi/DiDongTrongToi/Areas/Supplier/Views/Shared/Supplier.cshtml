                            }
                        }
                    </div>
                </li>
                <li class="nav-item navStyle " style="font-size: 80%;">
                    <a class="nav-link " href="#"><i class="fa fa-2x fa-cart-plus"></i><br />Giỏ Hàng</a>
                </li>
                <li class="nav-item navStyle " style="font-size: 80%;">
                    <a class="nav-link " href="#">
                        @if (ViewBag.adf == null)
                        {
                            <i class="fa fa-2x fa-user"></i>
                            <br />
                        }
                        else
                        {
                            string TaiKHoan = Convert.ToString(ViewBag.adf);
                            var img = db.thongTins.Where(n => n.taikhoan == TaiKHoan).FirstOrDefault();
                            if (img == null)
                            {
                                <i class="fa fa-2x fa-user"></i>
                                <br />
                            }
                            else
                            {
                                <img src="@img.avatar" style="height:2em; width:2em; border-radius:100%;" />
                                <br />
                            }
                        }
                        Đăng Nhập
                    </a>
                </li>
            </ul>
        </div>
    </nav>
    <div style="clear:both"></div>
    <div style="margin-top:100px;"></div>
    @RenderBody()
    <div class="clearfix"></div>
    <footer style="bottom:0; background-color:coral; width:100%; text-align:center;">
        © 2019.Di động Trong Tôi - designer <a href="#">HaiNam</a>
    </footer>
